# KinCircle Environment Variables
# Copy this file to .env.local and fill in your values

# Gemini AI API Key (get from https://makersuite.google.com/app/apikey)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Supabase Configuration (optional - for cloud sync)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Storage Provider: 'local' or 'supabase' (default: auto-detect)
VITE_STORAGE_PROVIDER=local
