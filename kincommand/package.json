{
  "name": "kincircle",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:supabase": "set E2E_SUPABASE=true&& playwright test e2e/supabase-sync.spec.ts --reporter=line",
    "test:e2e:real": "set E2E_GEMINI_REAL=true&& playwright test e2e/chat-real.spec.ts --reporter=line",
    "test:ops": "npm run test && npm run test:e2e",
    "gen-types": "supabase gen types typescript --project-id YOUR_PROJECT_ID > types/supabase.ts"
  },
  "dependencies": {
    "@google/genai": "^1.38.0",
    "@supabase/supabase-js": "^2.91.1",
    "lucide-react": "^0.563.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "recharts": "^3.7.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.1.2",
    "jsdom": "^27.4.0",
    "typescript": "~5.8.2",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
